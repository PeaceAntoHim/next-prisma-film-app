// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Food {
  id          Int      @default(autoincrement()) @id
  name        String?
  price       String?
  ingredients String?
  active      String?
  description String?
  imageUrl    String?
}